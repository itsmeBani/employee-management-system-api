{

  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,

    "allowImportingTsExtensions": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true

},

"include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
